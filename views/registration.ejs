    <!-- Contact -->

    <div class="contact_form_section">
        <div class="container">
            <div class="row">
                <div class="col">

                    <!-- Contact Form -->
                    <div class="contact_form_container">
                        <div class="contact_title text-center">fill the form</div>
                        <form action="/registersuccess" class="contact_form text-center" method="post">
                            <div>
                                <label for="fname"></label>
                                <input type="text" id="fname" class="contact_form_name input_field" placeholder="First Name" required="required" data-error="Name is required." name="credentials[fname]">
                            </div>
                            <div>
                                <label for="lname"></label>
                                <input type="text" id="lname" class="contact_form_name input_field" placeholder="Last Name" required="required" data-error="Name is required." name="credentials[lname]">
                            </div>
                            <div>
                                <label for="email"></label>
                                <input type="email" id="email" class="contact_form_name input_field" placeholder="Email" required="required" data-error="Email is required." name="credentials[email]">
                            </div>
                            <div>
                                <label for="phone"></label>
                                <input type="text" id="phone" class="contact_form_name input_field" placeholder="Phone Number" required="required" data-error="Number is required." name="credentials[phone]">
                            </div>
                            <div>
                                <label for="pass"></label>
                                <input type="password" id="pass" class="contact_form_name input_field" placeholder="Password (minimum length is 6 required)" minlength="6" required="required" data-error="Password is required." name="credentials[pass]">
                            </div>
                            <div>
                                <label for="confirmpass"></label>
                                <input type="password" id="confirmpass" class="contact_form_name input_field" placeholder="Retype Password" required="required" data-error="Password is required." name="credentials[confirmpass]">
                            </div>
                            <div>
                                <label for="dateofbirth"></label>
                                <input type="date" id="dateofbirth" class="contact_form_name input_field" placeholder="Date of Birth" required="required" data-error="Date of Birth Required" name="credentials[dateofbirth]">
                            </div>
                            <div class="form-group row-md-12">
                                <label for="category">Category</label>
                                <select id="category" class="form-control" required="required" data-error="Please Select" name="credentials[category]">
                                    <option>USER</option>
                                    <option>VOLUNTEER</option>
                                </select>
                            </div>
                            <div class="form-group row-md-12">
                                <label for="category">Category</label>
                                <select id="category" class="form-control" required="required" data-error="Please Select" name="credentials[gender]">
                                    <option>Male</option>
                                    <option>Female</option>
                                </select>
                            </div>
                            <button type="submit" id="form_submit_button" class="form_submit_button button trans_200">Register<span></span><span></span><span></span></button>
                        </form>
                    </div>

